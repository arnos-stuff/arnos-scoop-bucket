{
    "version": "0.1.0",
    "homepage": "https://github.com/arnos-stuff/pshazz-themes",
    "license": "MIT",
    "description": "Arno's functional programming themes for pshazz",
    "url": "https://github.com/arnos-stuff/pshazz-themes/archive/refs/heads/main.zip",
    "depends": [
        "pshazz",
        "7z"
    ],
    "installer": {
        "script" : [
            "$start_dir = pwd",
            "7z x $fname -o $original_dir",
            "cd $original_dir",
            "& '.\install.ps1'",
            "cd $start_dir"
        ]
    }
}
